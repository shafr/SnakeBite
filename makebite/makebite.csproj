  </PropertyGroup>
  <ItemGroup>
    <Reference Include="GzsTool.Core, Version=0.5.3.0, Culture=neutral, processorArchitecture=MSIL">
      <SpecificVersion>False</SpecificVersion>
      <HintPath>..\lib\GzsTool.Core.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Content Include="folder289.ico" />
